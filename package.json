    "passport-github": ">=0.1.5",
    "moment": "2.x",
    "optimist": "*",
    "github": "*",
    "memoizee": "~0.2.5",
    "promise": "~3.2.0"
  },
  "license": "MIT",
  "engines": {
    "node": "0.8.x",
    "npm": "1.1.x"
